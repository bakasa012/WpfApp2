<UserControl x:Class="Presentation.View.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Presentation.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <StackPanel>

            <DataGrid Height="200" x:Name="dtgExcel" SelectionChanged="dtgExcel_SelectionChanged"></DataGrid>

            <Button x:Name="btnImport" Click="btnImport_Click" Content="Import"></Button>

            <Button x:Name="btnExport" Click="btnExport_Click" Content="Export"></Button>
            <TextBlock Name="textBlock1" Text="DataTest"></TextBlock>
            <Button Content="DialogFileOpen" Click="Button_Click"/>
            <Button Content="ProcessExcel" Click="Button_Click_1"/>
            <TextBox Text="file path" Name="FileNameTextBox"></TextBox>
        </StackPanel>
    </Grid>
</UserControl>
